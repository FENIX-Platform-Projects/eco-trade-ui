<div class="container">
    <div class='row'>
        <div class='col-xs-12 col-sm-12 col-md-12 col-lg-12'>
            <h1>Geospatial Analysis</h1>
        </div>
    </div>
    <div class='row'>
        <div class='col-xs-12 col-sm-3 col-md-3 col-lg-3'>
            <h3>Indicator</h3>
            <select style="width:100%;" data-role="indicator"></select>
        </div>
        <div class='col-xs-12 col-sm-2 col-md-2 col-lg-1'>
            <h3>Year</h3>
            <select style="width:100%;" data-role="year">
                <option value="2015">2015</option>
                <option value="2010">2010</option>
                <option value="2005">2005</option>
                <option value="2000">2000</option>
                <!--<option value="1995">1995</option>-->
                <option value="1990">1990</option>
            </select>
        </div>
        <div class='col-xs-12 col-sm-3 col-md-3 col-lg-2'>
            <h3>Incomes</h3>
            <select style="width:100%;" data-role="incomes">
                <option value="">All Incomes</option>
                <option value="L">Low</option>
                <option value="LM">Lower Medium</option>
                <option value="UM">Upper Medium</option>
                <option value="H">High</option>
            </select>
        </div>
        <div class='col-xs-12 col-sm-3 col-md-3 col-lg-2'>
            <h3>Domain</h3>
            <select style="width:100%;" data-role="domain">
                <option value="">All Domains</option>
                <option value="Bor">Boreal</option>
                <option value="Pol">Polar</option>
                <option value="SubTrp">Subtropical</option>
                <option value="Trp">Tropical</option>
                <option value="Temp">Temperate</option>
            </select>
        </div>
    </div>

    <div class='row' style="padding-top:35px;">

        <div class='col-xs-12 col-sm-12 col-md-8 col-lg-12'>

            <div data-role="chart"></div>
        </div>
    </div>

<!--    <div class='row' style="padding-top:35px;">
        <div class='col-xs-12 col-sm-12 col-md-12 col-lg-12'>
            <div data-role="chart"></div>
        </div>
    </div>-->
</div>